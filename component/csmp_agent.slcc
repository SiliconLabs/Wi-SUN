  - path: "src/csmpagent/csmp_signature.c"
  - path: "src/csmpagent/tlvs/CsmpTlvs.pb-c.c"
  - path: "src/csmpservice/cgmsagent.c"
  - path: "src/csmpservice/csmpserver.c"
  - path: "src/csmpapi/csmpservice.c"
  - path: "src/lib/protobuf-c/protobuf-c.c"
  - path: "src/csmptlv/csmptlv.c"
  - path: "src/csmptlv/ProtobufVarint.c"
  - path: "src/coap/coapclient.c"
  - path: "src/coap/coapserver.c"
  

include:
  - path: "./include"
    file_list:
    - "path": "csmp_service.h"
    - "path": "csmp_info.h"


  - path: "./osal/efr32_wisun"
    file_list:
    - "path": "osal_platform_types.h"

  - path: "./osal"
    file_list:
    - "path": "osal.h"
  
  - path: "./sample"
    file_list:
    - "path": "CsmpAgentLib_sample_config.h"
    - "path": "signature_verify.h"
    - "path": "CsmpAgentLib_sample_util.h"
    - "path": "CsmpAgentLib_sample.h"
    - "path": "CsmpAgentLib_sample_tlvs.h"
  
  - path: "./src/csmpagent"
    file_list:
    - "path": "csmpagent.h"
    - "path": "csmp.h"
    - "path": "csmpfunction.h"

  - path: "./src/csmpagent/tlvs"
    file_list:
    - "path": "CsmpTlvs.pb-c.h"
  
  - path: "./src/csmpservice"
    file_list:
    - "path": "csmpserver.h"
    - "path": "cgmsagent.h"
  
  - path: "./src/csmpapi"
    file_list:
    - "path": "csmpinfo.h"
    - "path": "csmpservice.h"

  - path: "./src/lib"
    file_list:
    - "path": "debug.h"
  
  - path: "./src/lib/protobuf-c"
    file_list:
    - "path": "protobuf-c.h"
  
  - path: "./src/csmptlv"
    file_list:
    - "path": "ProtobufVarint.h"
