  - path: "src/csmpagent/csmp_currenttime.c"
  - path: "src/csmpagent/csmp_interfaceMetrics.c"
  - path: "src/csmpagent/csmp_firmwareImageInfo.c"
  - path: "src/csmpagent/csmp_hardwareDesc.c"
  - path: "src/csmpagent/csmp_deviceid.c"
  - path: "src/csmpagent/csmp_interfaceDesc.c"
  - path: "src/csmpagent/csmpagent.c"
  - path: "src/csmpagent/csmp_sessionId.c"
  - path: "src/csmpagent/csmp_group.c"
  - path: "src/csmpagent/csmp_ipRoute.c"
  - path: "src/csmpagent/csmp_vendor.c"
  - path: "src/csmpagent/csmp_cgms.c"
  - path: "src/csmpagent/csmp_rplInstance.c"
